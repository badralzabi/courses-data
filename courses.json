class Course {
  final String name;
  final String dossierDescription;
  final String summaryDescription;
  final String explanationDescription;
  final String pastQuestionsDescription;
  final String dossierLink;
  final String summaryLink;
  final String explanationLink;
  final String pastQuestionsLink;

  Course({
    required this.name,
    required this.dossierDescription,
    required this.summaryDescription,
    required this.explanationDescription,
    required this.pastQuestionsDescription,
    required this.dossierLink,
    required this.summaryLink,
    required this.explanationLink,
    required this.pastQuestionsLink,
  });

  // دالة لتحويل JSON إلى كائن Course
  factory Course.fromJson(Map<String, dynamic> json) {
    return Course(
      name: json['name'] ?? '',
      dossierDescription: json['dossierDescription'] ?? '',
      summaryDescription: json['summaryDescription'] ?? '',
      explanationDescription: json['explanationDescription'] ?? '',
      pastQuestionsDescription: json['pastQuestionsDescription'] ?? '',
      dossierLink: json['dossierLink'] ?? '',
      summaryLink: json['summaryLink'] ?? '',
      explanationLink: json['explanationLink'] ?? '',
      pastQuestionsLink: json['pastQuestionsLink'] ?? '',
    );
  }
}

